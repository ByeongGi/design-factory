{
  "$schema": "https://tr.designtokens.org/format/",
  "color": {
    "primary": {
      "$type": "color",
      "$value": "#1890ff",
      "$description": "Primary brand color - Daybreak Blue"
    },
    "primary-hover": {
      "$type": "color",
      "$value": "#40a9ff",
      "$description": "Primary hover state"
    },
    "primary-active": {
      "$type": "color",
      "$value": "#096dd9",
      "$description": "Primary active state"
    },
    "success": {
      "$type": "color",
      "$value": "#52c41a",
      "$description": "Success state color - Polar Green"
    },
    "warning": {
      "$type": "color",
      "$value": "#faad14",
      "$description": "Warning state color - Calendula Gold"
    },
    "error": {
      "$type": "color",
      "$value": "#f5222d",
      "$description": "Error state color - Dust Red"
    },
    "info": {
      "$type": "color",
      "$value": "#1890ff",
      "$description": "Info state color"
    },
    "text-primary": {
      "$type": "color",
      "$value": "rgba(0, 0, 0, 0.85)",
      "$description": "Primary text color"
    },
    "text-secondary": {
      "$type": "color",
      "$value": "rgba(0, 0, 0, 0.45)",
      "$description": "Secondary text color"
    },
    "text-disabled": {
      "$type": "color",
      "$value": "rgba(0, 0, 0, 0.25)",
      "$description": "Disabled text color"
    },
    "border": {
      "$type": "color",
      "$value": "#d9d9d9",
      "$description": "Default border color"
    },
    "background": {
      "$type": "color",
      "$value": "#ffffff",
      "$description": "Component background"
    },
    "background-light": {
      "$type": "color",
      "$value": "#fafafa",
      "$description": "Light background"
    }
  },
  "spacing": {
    "xs": {
      "$type": "dimension",
      "$value": "8px",
      "$description": "Extra small spacing"
    },
    "sm": {
      "$type": "dimension",
      "$value": "12px",
      "$description": "Small spacing"
    },
    "md": {
      "$type": "dimension",
      "$value": "16px",
      "$description": "Medium spacing"
    },
    "lg": {
      "$type": "dimension",
      "$value": "24px",
      "$description": "Large spacing"
    },
    "xl": {
      "$type": "dimension",
      "$value": "32px",
      "$description": "Extra large spacing"
    }
  },
  "typography": {
    "font-family": {
      "$type": "fontFamily",
      "$value": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
      "$description": "System font stack"
    },
    "font-size-sm": {
      "$type": "dimension",
      "$value": "12px",
      "$description": "Small font size"
    },
    "font-size-base": {
      "$type": "dimension",
      "$value": "14px",
      "$description": "Base font size"
    },
    "font-size-lg": {
      "$type": "dimension",
      "$value": "16px",
      "$description": "Large font size"
    },
    "font-size-xl": {
      "$type": "dimension",
      "$value": "20px",
      "$description": "Extra large font size"
    },
    "line-height": {
      "$type": "number",
      "$value": "1.5715",
      "$description": "Base line height"
    },
    "font-weight-normal": {
      "$type": "fontWeight",
      "$value": "400",
      "$description": "Normal font weight"
    },
    "font-weight-medium": {
      "$type": "fontWeight",
      "$value": "500",
      "$description": "Medium font weight"
    },
    "font-weight-semibold": {
      "$type": "fontWeight",
      "$value": "600",
      "$description": "Semibold font weight"
    }
  },
  "borderRadius": {
    "sm": {
      "$type": "dimension",
      "$value": "2px",
      "$description": "Small border radius"
    },
    "base": {
      "$type": "dimension",
      "$value": "4px",
      "$description": "Base border radius"
    },
    "lg": {
      "$type": "dimension",
      "$value": "8px",
      "$description": "Large border radius"
    }
  },
  "shadow": {
    "sm": {
      "$type": "shadow",
      "$value": "0 2px 8px rgba(0, 0, 0, 0.15)",
      "$description": "Small shadow"
    },
    "base": {
      "$type": "shadow",
      "$value": "0 3px 6px -4px rgba(0, 0, 0, 0.12), 0 6px 16px 0 rgba(0, 0, 0, 0.08), 0 9px 28px 8px rgba(0, 0, 0, 0.05)",
      "$description": "Base shadow"
    },
    "lg": {
      "$type": "shadow",
      "$value": "0 6px 16px -8px rgba(0, 0, 0, 0.08), 0 9px 28px 0 rgba(0, 0, 0, 0.05), 0 12px 48px 16px rgba(0, 0, 0, 0.03)",
      "$description": "Large shadow"
    }
  },
  "transition": {
    "duration": {
      "$type": "duration",
      "$value": "0.3s",
      "$description": "Default transition duration"
    },
    "timing-function": {
      "$type": "cubicBezier",
      "$value": "cubic-bezier(0.645, 0.045, 0.355, 1)",
      "$description": "Ant Design motion curve"
    }
  }
}
